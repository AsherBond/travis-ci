<table id="builds">
  <thead>
    <tr>
      <th>Build</th>
      <th>Commit</th>
      <th>Message</th>
      <th>Duration</th>
      <th>Finished</th>
    </tr>
  </thead>

  {{#collection SC.TemplateCollectionView tagName="tbody" itemClass="build" contentBinding="Travis.buildsTabController"}}
    <td class="number"><a {{bindAttr href="content.urlBuild"}}>{{content.number}}</a></td>
    <td class="commit"><a {{bindAttr href="content.urlGithubCommit"}}>{{content.commit}}{{#if content.branch}} ({{content.branch}}){{/if}}</a></td>
    <td class="message">{{content.message}}</td>
    <td class="duration" {{bindAttr title="content.duration"}}></td>
    <td class="finished_at timeago" {{bindAttr title="content.finished_at"}}>{{#if content.finished_at}}{{content.finished_at}}{{else}}-{{/if}}</td>
  {{/collection}}
</table>

